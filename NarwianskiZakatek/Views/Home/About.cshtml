@model NarwianskiZakatek.Models.Description

@{
    ViewData["Title"] = "O nas";
}

<h1>@Html.DisplayFor(model => model.Title)</h1>
<br />
<div class="mapouter">
    <div class="gmap_canvas">
        <iframe width="800" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=Konowa%C5%82y%2040&t=&z=13&ie=UTF8&iwloc=&output=embed"
                frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
        <a href="https://www.embedgooglemap.net/blog/divi-discount-code-elegant-themes-coupon/">divi discount</a>
        <br>
        <style>
            .mapouter {
                position: relative;
                text-align: right;
                height: 500px;
                width: 800px;
            }</style>
        <a href="https://www.embedgooglemap.net">google map embed</a>
        <style>
            .gmap_canvas {
                overflow: hidden;
                background: none !important;
                height: 500px;
                width: 800px;
            }
        </style>
    </div>
</div>
<br />
<p>
    @Html.DisplayFor(model => model.Content)
</p>
<br />
<img src="@Model.getFullPhotoPath()" width="200" />