services:
  adminer:
    depends_on:
      postgres:
        condition: service_started
    image: adminer
    ports:
    - published: 8080
      target: 8080
    restart: always
  narwianskizakatek:
    build:
      context: C:\Users\leszc\Desktop\NarwianskiZakatek
      dockerfile: NarwianskiZakatek/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: NarwianskiZakatek
      target: base
    container_name: NarwianskiZakatek
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: narwianskizakatek:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/NarwianskiZakatek.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\sleszczynski\Desktop\NarwianskiZakatek\NarwianskiZakatek:/app:rw
    - C:\Users\sleszczynski\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\leszc\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\leszc\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\sleszczynski\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\sleszczynski\Desktop\NarwianskiZakatek:/src:rw
  postgres:
    environment:
      POSTGRES_DB: myDataBase
      POSTGRES_PASSWORD: postgrespw
      POSTGRES_USER: postgres
    image: postgres:latest
    ports:
    - published: 5432
      target: 5432
    restart: always
    volumes:
    - C:\Users\leszc\Desktop\NarwianskiZakatek\data:/var/lib/postgresql:rw
version: '3.4'